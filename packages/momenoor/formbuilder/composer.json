{
    "name": "momenoor/formbuilder",
    "description": "Build a form builder form laravel projects, in a simple way.",
    "type": "library",
    "require": {
        "spatie/laravel-html": "dev-main"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Momenoor\\FormBuilder\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Momen Noor",
            "email": "momen.noor@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Momenoor\\FormBuilder\\FormBuilderServiceProvider"
            ],
            "aliases": {
                "FormBuilder": "Momenoor\\FormBuilder\\Facades\\FormBuilder"
            }
        }
    },
    "minimum-stability": "dev"
}
